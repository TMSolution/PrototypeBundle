{% extends 'WebProfilerBundle:Profiler:layout.html.twig' %}

{% block toolbar %}
    {% set icon %}
        <span class="icon"><img src="data:image/png;base64," alt=""/></span>
        <span class="sf-toolbar-status">Prototype</span>
    {% endset %}

    {% set text %}
    <div>URI: {{ collector.data.uri }}</div>
    <div>Route: {{ collector.data.route }}</div>
    <div>Controller: {{ collector.data.controller }}</div>
    <div>Locale: {{ collector.data.locale }}</div>
    <div>EntityName: {{ collector.data.entityName }}</div>
    <div>EntityClass: {{ collector.data.entityClass }}</div>    
    {% endset %}

    {{ include('@WebProfiler/Profiler/toolbar_item.html.twig', { 'link': true }) }}

{% endblock %}

{% block head %}
    {{ parent() }}    
{% endblock %}

{% block menu %}
    <span class="label">
{#       <span class="icon"><img src="data:image/svg+xml;base64," alt=""/></span>
#}        <strong>Prototype</strong>
    </span>
{% endblock %}

{% block panel %}
    {# Optional, for showing the most details. #}
    <h1>Prototype</h1>
    <h2>{{ collector.data.header }}</h2>
    <div><strong>URI</strong>: {{ collector.data.uri }}</div>
    <div><strong>Route</strong>: {{ collector.data.route }}</div>
    <div><strong>Controller</strong>: {{ collector.data.controller }}</div>
    <div><strong>Locale</strong>: {{ collector.data.locale }}</div>
    <div><strong>EntityName</strong>: {{ collector.data.entityName }}</div>
    <div><strong>EntityClass</strong>: {{ collector.data.entityClass }}</div>
    
    {% for line in collector.data.config %}
        <div>{{ line | raw }}</div>    
    {% endfor %}

    {% for line in collector.data.twigs %}
        <div>{{ line | raw  }}</div>    
    {% endfor %}

    
    {% for line in collector.data.gridconfig %}
        <div>{{ line | raw  }}</div>    
    {% endfor %}
    
    {% for line in collector.data.formtype %}
        <div>{{ line | raw  }}</div>    
    {% endfor %}

{% endblock %}

{% block panel %}
    {# Optional, for showing the most details. #}
    <h1>Prototype</h1>
    <h2>{% if collector.data.header is defined %}{{ collector.data.header }}{% else %}none{% endif %}</h2>
    <div><strong>URI</strong>: {% if collector.data.uri is defined %}{{ collector.data.uri }}{% else %}none{% endif %}</div>
    <div><strong>Route</strong>: {% if collector.data.route is defined %}{{ collector.data.route }}{% else %}none{% endif %}</div>
    <div><strong>Controller</strong>: {% if collector.data.controller is defined %}{{ collector.data.controller }}{% else %}none{% endif %}</div>
    <div><strong>Locale</strong>: {% if collector.data.locale is defined %}{{ collector.data.locale }}{% else %}none{% endif %}</div>
    <div><strong>EntityName</strong>: {% if collector.data.entityName is defined %}{{ collector.data.entityName }}{% else %}none{% endif %}</div>
    <div><strong>EntityClass</strong>: {% if collector.data.entityClass is defined %}{{ collector.data.entityClass }}{% else %}none{% endif %}</div>


    {% if collector.data.config is defined %}
        {% for line in collector.data.config %}
            {{ line | raw }}   
        {% endfor %}
    {% endif %}

    {% if collector.data.twigs is defined %}
        {% for line in collector.data.twigs %}
            {{ line | raw  }}   
        {% endfor %}
    {% endif %}

    {% if collector.data.gridconfig is defined %}
        {% for line in collector.data.gridconfig %}
            {{ line | raw  }}   
        {% endfor %}
    {% endif %}

    {% if collector.data.formtype is defined %}
        {% for line in collector.data.formtype %}
            {{ line | raw  }}   
        {% endfor %}
    {% endif %}
{% endblock panel %}
